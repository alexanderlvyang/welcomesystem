<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.yang.main.mapper.RoleMenuMapper">
    <insert id="addRoleMenu" parameterType="org.yang.main.pojo.RoleMenu">
        insert into role_menu values(default,#{roleid},#{menuid})
    </insert>
    <delete id="delRoleMenun" parameterType="Integer">
        delete from role_menu where roleid=1 and menuid=#{id}
    </delete>



</mapper>